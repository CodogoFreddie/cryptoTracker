{
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-plugin-node-source-map-support": "^0.0.1",
		"babel-preset-codogo": "^1.0.11",
		"babel-preset-env": "^1.5.1",
		"babel-register": "^6.24.1"
	},
	"babel": {
		"presets": [
			"env"
		],
		"plugins": [
			"transform-object-rest-spread",
			"syntax-trailing-function-commas",
			"babel-plugin-node-source-map-support"
		]
	},
	"scripts": {
		"make": "make lib -j 4",
		"start": "yarn run make && yarn run run-script",
		"remotedev": "remotedev --hostname=localhost --port=3000",
		"watch": "yarn make lib && watchman-make -p \"package.json\" \"makefile\" \"src/**/*.js\" -t run"
	},
	"dependencies": {
		"moment": "^2.18.1",
		"node-fetch": "^1.7.0",
		"ramda": "^0.24.0",
		"redux": "^3.6.0",
		"redux-logger": "^3.0.6",
		"redux-persist": "^4.8.0",
		"redux-persist-node-storage": "^1.0.2"
	}
}
